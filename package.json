{
  "name": "poplar",
  "version": "0.2.0",
  "description": "Electron menubar app for controlling a wemo device",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --cache ./src/ || true",
    "prebuild": "rm -rf ./app && rm -rf ./build && webpack --config ./config/webpack.config.prod.js -p --progress",
    "build": "electron-packager . WemoMenubarController --platform=darwin --arch=x64 --prune=true --out=build",
    "start": "electron .",
    "precompile-dev": "rm -rf ./app",
    "compile-dev": "webpack --config ./config/webpack.config.dev.js --progress || true",
    "dev-watch": "webpack --config ./config/webpack.config.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryexley/poplar.git"
  },
  "author": "Bob Yexley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryexley/poplar/issues"
  },
  "homepage": "https://github.com/ryexley/poplar#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-rpc": "^2.0.1",
    "lodash": "^4.17.4",
    "menubar": "^5.2.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "wemo-client": "^0.11.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.1.11",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react-app": "^2.1.1",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "eslint": "^3.16.1",
    "eslint-config-leankit": "^3.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  }
}
